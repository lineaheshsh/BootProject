                                                <div class="ml-4">
                                                    <div class="small"><a href="#">{{title}}</a></div>
                                                    <div class="text-xs text-muted">{{domain}}</div>
                                                </div>
                                            </div>
                                            <div class="ml-4 small">
                                                <div class="badge badge-light mr-3">{{date}} {{ampm}} {{time}}</div>
                                            </div>
