                            </li>
                        </ul>
                    </div>
                    <div class="card-body">
                        <div class="tab-content" id="dashboardNavContent">
                            <!-- Dashboard Tab Pane 1-->
                            <div class="tab-pane fade active show" id="overview" role="tabpanel" aria-labelledby="overview-pill">

                                {{#politicsTotal}}
